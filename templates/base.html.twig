{# templates/base.html.twig #}
<!DOCTYPE html>
<html>
<head>


    <meta charset="UTF-8">
    <title>{% block title %}Contactor{% endblock %}</title>


    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}

    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}


</head>
<body>


{% block body %}
    <div id="sidebar">
        {% block sidebar %}
            <ul>
                <li><a href="{{ path('contacts_all') }}">Home</a></li>
            </ul>
        {% endblock %}
    </div>

    <div id="content">
        {% block content %}{% endblock %}
    </div>
{% endblock %}


</body>
</html>

